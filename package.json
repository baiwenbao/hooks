{
  "name": "@baiwenbao/hooks",
  "version": "1.1.24",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "_build": "father-build",
    "_deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "_prepublishOnly": "npm run build",
    "build:esm": "tsc",
    "build:watch": "tsc --watch"
  },
  "repository": {
    "url": "https://github.com/baiwenbao/hooks"
  },
  "module": "esm/index.js",
  "main": "./esm/index.js",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ahooks": "^3.1.8",
    "async-validator": "^3.5.1",
    "immutability-helper": "^3.1.1",
    "js-cookie": "^2.2.1",
    "lodash-es": "^4.17.21",
    "mathjs": "^8.1.1",
    "query-string": "^5.1.1",
    "rxjs": "^6.5.5",
    "rxjs-hooks": "^0.6.2",
    "tapable": "^2.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/js-cookie": "^2.2.5",
    "@types/lodash-es": "^4.17.5",
    "@types/mathjs": "^6.0.10",
    "@types/mockjs": "^1.0.2",
    "@types/query-string": "^5.1.0",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "antd": "4",
    "axios": "^0.18.0",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "prettier": "^2.2.1",
    "react": "^16.12.0 || ^17.0.0",
    "react-router-dom": "^5.0.1",
    "tslib": "^2.3.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd": "^4",
    "axios": "^0.18.0",
    "moment": "^2.27.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "typescript": "^3.8.3"
  }
}
